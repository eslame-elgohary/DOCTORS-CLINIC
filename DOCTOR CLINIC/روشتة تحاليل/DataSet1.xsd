                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        R_T_DOCTOR.*, R_T_DOCTOR_DT.R_DT_CODE, R_T_DOCTOR_DT.R_DT_THLEL, PATIENT.PA_NAME, PATIENT.PA_TEL, PATIENT.PA_AGE, PATIENT.PA_TYPE, 
